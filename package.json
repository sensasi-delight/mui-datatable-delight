{
    "name": "mui-datatable-delight",
    "version": "0.0.0-experimental.9",
    "description": "A responsive DataTable component built with MUI for React-based project.",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "type": "module",
    "files": [
        "dist"
    ],
    "scripts": {
        "test": "cross-env NODE_ENV=test mocha --require @babel/register test/**/*.test.js",
        "coverage": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text mocha --require @babel/register test/*.js && nyc report --reporter=html | cat coverage/lcov.info | coveralls",
        "coverage:html": "cross-env NODE_ENV=test nyc check-coverage --lines 55 --reporter=html --reporter=text mocha --require @babel/register test/*.js && nyc report --reporter=html",
        "dev": "next docs --turbopack",
        "prettier": "prettier './**/*.{js,jsx,ts,tsx,mjs,cjs}' --write",
        "lint": "eslint src",
        "build": "rollup -c",
        "build:docs": "next build docs",
        "prepublishOnly": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/sensasi-delight/mui-datatable-delight.git"
    },
    "keywords": [
        "material-ui",
        "datatables",
        "react"
    ],
    "author": "gregnb <gregnb@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/gregnb/mui-datatable-delight/issues"
    },
    "devDependencies": {
        "@emotion/cache": "^11.14.0",
        "@eslint/js": "^9.17.0",
        "@fontsource/roboto": "^5.1.0",
        "@mdx-js/loader": "^3.1.0",
        "@mdx-js/react": "^3.1.0",
        "@mui/material-nextjs": "^6.3.0",
        "@next/mdx": "^15.1.3",
        "@rollup/plugin-commonjs": "^28.0.2",
        "@rollup/plugin-node-resolve": "^16.0.0",
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^12.1.2",
        "@types/mdx": "^2.0.13",
        "@types/mui-datatables": "^4.3.12",
        "@types/react": "^19.0.2",
        "@types/react-dom": "^19.0.2",
        "chai": "^4.2.0",
        "coveralls": "^3.1.0",
        "cross-env": "^7.0.2",
        "enzyme": "^3.11.0",
        "eslint": "^9.17.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-filenames": "^1.3.2",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-react": "^7.37.2",
        "eslint-plugin-react-hooks": "^5.1.0",
        "eslint-plugin-react-refresh": "^0.4.16",
        "jsdom": "^16.2.2",
        "jsdom-global": "^3.0.2",
        "mocha": "^7.2.0",
        "next": "^15.1.3",
        "nyc": "^15.0.1",
        "prettier": "^3.4.2",
        "prismjs": "^1.29.0",
        "raw-loader": "^4.0.1",
        "react": "^18.3.1",
        "react-dnd-test-backend": "^11.1.3",
        "react-dnd-test-utils": "^11.1.3",
        "react-dom": "^18.3.1",
        "react-waypoint": "^10.1.0",
        "rollup": "^4.29.1",
        "rollup-plugin-delete": "^2.1.0",
        "rollup-plugin-filesize": "^10.0.0",
        "rollup-pluginutils": "^2.8.2",
        "rollup-preserve-directives": "^1.1.3",
        "simulant": "^0.2.2",
        "sinon": "^7.4.2",
        "typescript": "^5.7.2",
        "typescript-eslint": "^8.18.2"
    },
    "dependencies": {
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.0",
        "@mui/icons-material": "^6.2.1",
        "@mui/material": "^6.2.1",
        "clsx": "^2.1.1",
        "prop-types": "^15.7.2",
        "react-dnd": "^11.1.3",
        "react-dnd-html5-backend": "^11.1.3",
        "react-to-print": "^2.8.0",
        "tss-react": "^4.9.14"
    },
    "side-effects": false,
    "nyc": {
        "lines": 50,
        "statements": 50,
        "functions": 50,
        "branches": 50,
        "check-coverage": true,
        "all": true,
        "include": [
            "src/**/*.js"
        ],
        "exclude": [
            "test/**/*.test.js"
        ],
        "sourceMap": false,
        "instrument": false
    }
}
