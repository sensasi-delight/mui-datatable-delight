{
  "name": "mui-datatable-delight",
  "description": "Datatables for React using Material-UI",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "envp": "cross-env NODE_ENV=production",
    "envd": "cross-env NODE_ENV=development",
    "dev": "npm run envd && vite --force",
    "test": "cross-env NODE_ENV=test mocha --require @babel/register test/**/*.test.js",
    "docs:dev": "next docs",
    "docs:build": "npm run envp && next build docs",
    "docs:export": "next export docs -o docs/export",
    "docs:deploy": "npm run docs:build && npm run docs:export && firebase deploy",
    "docs:start": "next start docs",
    "coverage": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text mocha --require @babel/register test/*.js && nyc report --reporter=html | cat coverage/lcov.info | coveralls",
    "coverage:html": "cross-env NODE_ENV=test nyc check-coverage --lines 55 --reporter=html --reporter=text mocha --require @babel/register test/*.js && nyc report --reporter=html",
    "prettier": "prettier './**/*.{js,jsx,ts,tsx,mjs,cjs}' --write",
    "lint": "eslint src",
    "build": "npm run envp && npm run prettier && rollup -c",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sensasi-delight/mui-datatable-delight.git"
  },
  "keywords": [
    "material-ui",
    "datatables",
    "react"
  ],
  "author": "gregnb <gregnb@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gregnb/mui-datatable-delight/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-external-helpers": "^7.10.1",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.1",
    "@babel/plugin-transform-async-to-generator": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/register": "^7.10.1",
    "@babel/runtime-corejs3": "^7.12.1",
    "@rollup/plugin-babel": "5.0.2",
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.3.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/mui-datatables": "^4.3.12",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "chai": "^4.2.0",
    "core-js": "^3.6.5",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "enzyme": "^3.11.0",
    "eslint": "^7.1.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "globals": "^15.14.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^16.2.2",
    "jsdom-global": "^3.0.2",
    "mocha": "^7.2.0",
    "next": "^13.5.7",
    "nyc": "^15.0.1",
    "prettier": "^3.4.2",
    "prismjs": "^1.20.0",
    "raw-loader": "^4.0.1",
    "react-dnd-test-backend": "^11.1.3",
    "react-dnd-test-utils": "^11.1.3",
    "react-router": "^7.1.0",
    "react-waypoint": "^10.1.0",
    "rollup": "^2.3.4",
    "rollup-plugin-filesize": "^10.0.0",
    "rollup-pluginutils": "^2.8.2",
    "simulant": "^0.2.2",
    "sinon": "^7.4.2",
    "style-loader": "^1.2.1",
    "typescript": "^5.7.2",
    "vite": "^6.0.5",
    "vite-plugin-babel": "^1.3.0"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^5.16.12",
    "@mui/material": "^5.16.12",
    "clsx": "^1.1.1",
    "lodash.assignwith": "^4.2.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.debounce": "^4.0.8",
    "lodash.find": "^4.6.0",
    "lodash.get": "^4.4.2",
    "lodash.isequal": "^4.5.0",
    "lodash.isundefined": "^3.0.1",
    "lodash.memoize": "^4.1.2",
    "lodash.merge": "^4.6.2",
    "prop-types": "^15.7.2",
    "react": "^18.3.1",
    "react-dnd": "^11.1.3",
    "react-dnd-html5-backend": "^11.1.3",
    "react-dom": "^18.3.1",
    "react-sortable-tree-patch-react-17": "^2.9.0",
    "react-to-print": "^2.8.0",
    "tss-react": "^3.6.0"
  },
  "side-effects": false,
  "nyc": {
    "lines": 50,
    "statements": 50,
    "functions": 50,
    "branches": 50,
    "check-coverage": true,
    "all": true,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "test/**/*.test.js"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
